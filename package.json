{
  "name": "landing",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "useTabs": false,
    "endOfLine": "lf",
    "singleQuote": true,
    "bracketSpacing": true,
    "trailingComma": "none",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "deviceful": "^0.9.4",
    "randomstring": "^1.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.11.0",
    "react-scripts": "3.4.3",
    "react-snap": "^1.23.0",
    "react-spring": "^8.0.27",
    "react-three-fiber": "^4.2.20",
    "three": "^0.119.1",
    "three.meshline": "^1.2.0",
    "threejs-meshline": "^2.0.11"
  },
  "devDependencies": {
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.0.5"
  }
}
