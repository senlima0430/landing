{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "dev": "NODE_ENV=development TAILWIND_MODE=watch vite",
    "build": "tsc && NODE_ENV=production TAILWIND_MODE=build vite build",
    "serve": "vite preview"
  },
  "prettier": {
    "semi": false,
    "tabWidth": 2,
    "printWidth": 78,
    "useTabs": false,
    "endOfLine": "lf",
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "jsxBracketSameLine": false
  },
  "dependencies": {
    "framer-motion": "^4.0.3",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@tailwindcss/jit": "^0.1.17",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.8",
    "prettier": "^2.1.2",
    "tailwindcss": "^2.0.4",
    "typescript": "^4.1.2",
    "vite": "^2.1.3"
  }
}
